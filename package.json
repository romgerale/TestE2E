{
  "name": "test-e2e-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test ",
    "lint": "ng lint",
    "mock-server": "npm --prefix mock-server run start",
    "testcoverage": "ng test --no-watch --code-coverage",
    "alldevtests" : "npm run testcoverage && npm run integratedmockedtests",
    "integratedmockedtests": "concurrently --kill-others --success first \"npm run mock-server\" \"npm run ng e2e -- --suite=core\"",
    "integratedhottests" : "npm run ng e2e -- --suite=core --c integratedhot",
    "fulle2etests" : "npm run ng e2e -- --suite=critical --c fulle2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "concurrently": "^6.0.2",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.0.0",
    "@angular/cli": "~7.3.0",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2",
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/cucumber": "^6.0.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cucumber": "^6.0.5",
    "protractor-cucumber-framework": "^6.2.1",
    "protractor-multiple-cucumber-html-reporter-plugin": "^1.8.1",
    "cucumber-junit-formatter": "^0.2.2",
    "jasmine-reporters": "2.3.2",
    "nyc": "~13.1.0",
    "ngx-build-plus": "~7.5.0",
    "npm-run-all": "^4.1.5",
    "xslt-processor": "^0.11.5"
  }
}
